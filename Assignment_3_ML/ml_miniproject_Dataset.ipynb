   "source": [
    "import gzip\n",
    "\n",
    "x = 0\n",
    "\n",
    "with gzip.open('yelp_dataset.json.gz', 'rb+') as f:\n",
    "    file_content = f.readlines()\n",
    "\n",
    "\n",
    "g = open('yelp_dataset_Initial.txt', 'w+')\n",
    "\n",
    "while x < len(file_content):\n",
    "    g.write(str(file_content[x]))\n",
    "    x += 1"
   ]
